<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<TitledPane xmlns:fx="http://javafx.com/fxml/1" text="%label-pane-title-actions" xmlns="http://javafx.com/javafx/8.0.60"
            fx:controller="com.vsegouin.mowitnow.ui.gui.controllers.actions.MapAction">
    <content>
        <VBox xmlns:fx="http://javafx.com/fxml/1">
            <Button fx:id="addMowerButton" onAction="#prepareAddMower" prefWidth="110.0" text="%add-mower">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
            <Button fx:id="removeMowerButton" onAction="#prepareRemoveMower" prefWidth="110.0" text="%remove-mower">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </VBox.margin>
            </Button>
        </VBox>
    </content>
</TitledPane>
